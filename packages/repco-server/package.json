    "multiformats": "^9.9.0",
    "pino": "^8.7.0",
    "pino-pretty": "^9.1.1",
    "repco-core": "0.1.0",
    "repco-graphql": "0.1.0",
    "repco-common": "0.1.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "nodemon": "^2.0.19",
    "tslog": "^3.3.4",
    "typescript": "^4.7.4"
  }
}
