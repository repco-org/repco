  "license": "MIT OR Apache-2.0",
  "maintainers": [
    "repco contributors"
  ],
  "main": "./dist/src/lib.js",
  "type": "module",
  "scripts": {
    "build": "tsc --build",
    "watch": "tsc --build -w",
